<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.michaelcapawana.fitness">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity_goals" />
        <activity android:name=".activity_schedule" />
        <activity android:name=".activity_graphs" />
        <activity android:name=".activity_exercise" />
        <activity android:name=".activity_userInfo" />
        <activity android:name=".cardio" />
        <activity android:name=".activity_core"></activity>
    </application>

</manifest>